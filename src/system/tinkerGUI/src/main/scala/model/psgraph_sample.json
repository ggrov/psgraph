{
	"ps_graph":{
		
		/* THE CURRENTLY VIEWED GRAPH NAME */
		"current":"nested", // if name = main then main graph

		/* THE CURRENTLY VIEWED GRAPH INDEX */
		"current_index": 0,

		/* THE MAIN GRAPH OBJECT, GENERATED BY QUANTO (SIGNATURE TO BE CHANGED WHEN .qtheory FILE WILL BE UPDATED) */
		"graph":{
			"dir_edges":{ // list of the edges
				"e1":{ // edge id
					"data":{ // edge data
						"gtyp_name":"any", // the goal types
						"type":"GT" // type of edge ?
					},
					"src":"v0", // source vertex
					"tgt":"v1" // target vertex
				},
				"e0":{ // ...
				},
			},
			"wire_vertices":{ // list of the boundaries
				"b1":{ // boundary id
					"annotation":{ // boundary data
						"boundary":true, // is it a boundary
						"coord":[1.1,-3.55] // boundary coordinates
					}
				},
				"b0":{ // ...
				}
			},
			"node_vertices":{ // list of the interesting nodes
				"v1":{ // node id
					"data":{ // node data
						"atm":"tac1", // node name
						"type":"T_Atomic", // type id (RT_ATM/RT_ID/RT_NST)
						"label":"tac1(x, y: b a)"
					},
					"annotation":{
						"coord":[-1.14,1.51] // node coordinates
					}
				},
				"v0":{ // ...
				}
			}
		},

		/* LIST OF ALL THE "SUB-GRAPHS" */
		"graph_tactics":[
			{
				"name":"thing", // name of the tactic
				"args":[ // list of the arguments
					[ // one argument : list of string
						"string",
						"string2"
					],[
						// ...
					]
				],
				"isOr":true, // applying OR or ORELSE
				"graphs":[ // list of the possible graphs
					{
						// same signature as main graph (generated by quanto)
					},{
						// ...
					}
				]
			},{
				// ...
			}
		],

		/* LIST OF THE ATOMIC TACTICS */
		"atomic_tactics":[
			{
				"name":"tac2", // the name
				"args":[ // same as graph tactics arguments
				],
				"tactic":"simplify", // the tactic
				"occurences":[["nested","v1"],"v0"]
			},
			{
				"name":"tac1", // the name
				"args":[["x"],["y","b","a"] // same as graph tactics arguments
				],
				"tactic":"simplify" // the tactic
			},{
				// ...
			}
		],

		/* GOAL TYPE STRING, WILL BE PARSE ON ML SIDE */
		"goal_types": "..."
	}
}